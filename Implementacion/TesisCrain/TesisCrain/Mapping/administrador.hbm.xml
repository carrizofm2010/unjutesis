<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TesisCrain"
                   namespace="TesisCrain.Dominio">
  <!--<class name="TesisCrain.Categoria, TesisCrain" table="categoria">-->
  <class name="Administrador" table="administrador" lazy="true" >
    <id name="IdAdministrador" column="ADMINISTRADOR_ID"  type="int" unsaved-value="0">
      <generator class="native"/>
    </id>
    
    <property name="NombreAdministrador" column="ADMINISTRADOR_NOMBRE" type="String">
    </property>
    <property name="ApellidoAdministrador" column="ADMINISTRADOR_APELLIDO" type="String">
    </property>
    <property name="UsuarioAdministrador" column="ADMINISTRADOR_USUARIO" type="String">
    </property>
    <property name="PasswordAdministrador" column="ADMINISTRADOR_PASSWORD" type="String">
    </property>
    <property name="CargoAdministrador" column="ADMINISTRADOR_CARGO" type="String">
    </property>
    
  </class>

</hibernate-mapping>

